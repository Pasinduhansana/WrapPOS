<Window x:Class="WrapPOS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WrapPOS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <!--#region Header Page Radio Buttons -->
        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="2" HorizontalAlignment="Right" Margin="0,8,141,0">
            <RadioButton x:Name="Settings_rd" Content="Settings" FontSize="11" Style="{DynamicResource MaterialDesignFlatLightButton}" Foreground="#FF323232" />
        </StackPanel>
        <!--#endregion-->

    </Grid>
</Window>
